TitleBadgeMixin from '../mixins/titleBadge'\n\nexport default {\n  mixins: [TitleBadgeMixin],\n  data() {\n    return {\n      postsList: [],\n      countByYear: {}, // 根据年份统计的文章数\n\n      perPage: 80, // 每页长\n      currentPage: 1// 当前页\n\n    }\n  },\n  created() {\n    this.getPageData()\n\n    // 根据年份计算出文章数\n    const { $sortPostsByDate, countByYear } = this\n    for (let i = 0; i < $sortPostsByDate.length; i++) {\n      const { frontmatter: { date } } = $sortPostsByDate[i];\n      if (date && type(date) === 'string') {\n        const year = date.slice(0, 4)\n        if (!countByYear[year]) {\n          countByYear[year] = 0\n        }\n        countByYear[year] = countByYear[year] + 1\n      }\n    }\n    this.countByYear = countByYear\n  },\n  mounted() {\n    window.addEventListener('scroll', debounce(() => {\n      if (this.postsList.length < this.$sortPostsByDate.length) {\n        const docEl = document.documentElement\n        const docBody = document.body\n        const scrollTop = docEl.scrollTop || docBody.scrollTop;\n        const clientHeight = docEl.clientHeight || docBody.clientHeight;\n        const scrollHeight = docEl.scrollHeight || docBody.scrollHeight;\n\n        if (scrollHeight > clientHeight && scrollTop + clientHeight >= scrollHeight - 250) {\n          this.loadmore()\n        }\n      }\n\n    }, 200))\n  },\n  methods: {\n    getPageData() {\n      const currentPage = this.currentPage\n      const perPage = this.perPage\n      this.postsList = this.postsList.concat(this.$sortPostsByDate.slice((currentPage - 1) * perPage, currentPage * perPage))\n    },\n    loadmore() {\n      this.currentPage = this.currentPage + 1\n      this.getPageData()\n    },\n    getYear(index) {\n      const item = this.postsList[index]\n      if (!item) {\n        return\n      }\n      const { frontmatter: { date } } = item\n      if (date && type(date) === 'string') {\n        return date.slice(0, 4)\n      }\n    },\n    getDate(item) {\n      const { frontmatter: { date } } = item\n      if (date && type(date) === 'string') {\n        return date.slice(5, 10)\n      }\n    }\n  }\n}\n"],"mappings":"AACA,OAAOA,QAAQ,MAAM,iBAAiB;AACtC,SAASC,IAAI,QAAQ,SAAS;AAC9B,OAAOC,eAAe,MAAM,sBAAsB;AAElD,eAAe;EACbC,MAAM,EAAE,CAACD,eAAe,CAAC;EACzBE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,CAAC,CAAC;MAAE;;MAEjBC,OAAO,EAAE,EAAE;MAAE;MACbC,WAAW,EAAE,CAAC;IAEhB,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;;IAElB;IACA,MAAM;MAAEC,gBAAgB;MAAEL;IAAY,CAAC,GAAG,IAAI;IAC9C,KAAK,IAAIM,CAAC,GAAG,CAAC,EAAEA,CAAC,GAAGD,gBAAgB,CAACE,MAAM,EAAED,CAAC,EAAE,EAAE;MAChD,MAAM;QAAEE,WAAW,EAAE;UAAEC;QAAK;MAAE,CAAC,GAAGJ,gBAAgB,CAACC,CAAC,CAAC;MACrD,IAAIG,IAAI,IAAId,IAAI,CAACc,IAAI,CAAC,KAAK,QAAQ,EAAE;QACnC,MAAMC,IAAI,GAAGD,IAAI,CAACE,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;QAC7B,IAAI,CAACX,WAAW,CAACU,IAAI,CAAC,EAAE;UACtBV,WAAW,CAACU,IAAI,CAAC,GAAG,CAAC;QACvB;QACAV,WAAW,CAACU,IAAI,CAAC,GAAGV,WAAW,CAACU,IAAI,CAAC,GAAG,CAAC;MAC3C;IACF;IACA,IAAI,CAACV,WAAW,GAAGA,WAAW;EAChC,CAAC;EACDY,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEpB,QAAQ,CAAC,MAAM;MAC/C,IAAI,IAAI,CAACK,SAAS,CAACQ,MAAM,GAAG,IAAI,CAACF,gBAAgB,CAACE,MAAM,EAAE;QACxD,MAAMQ,KAAK,GAAGC,QAAQ,CAACC,eAAe;QACtC,MAAMC,OAAO,GAAGF,QAAQ,CAACG,IAAI;QAC7B,MAAMC,SAAS,GAAGL,KAAK,CAACK,SAAS,IAAIF,OAAO,CAACE,SAAS;QACtD,MAAMC,YAAY,GAAGN,KAAK,CAACM,YAAY,IAAIH,OAAO,CAACG,YAAY;QAC/D,MAAMC,YAAY,GAAGP,KAAK,CAACO,YAAY,IAAIJ,OAAO,CAACI,YAAY;QAE/D,IAAIA,YAAY,GAAGD,YAAY,IAAID,SAAS,GAAGC,YAAY,IAAIC,YAAY,GAAG,GAAG,EAAE;UACjF,IAAI,CAACC,QAAQ,CAAC,CAAC;QACjB;MACF;IAEF,CAAC,EAAE,GAAG,CAAC,CAAC;EACV,CAAC;EACDC,OAAO,EAAE;IACPpB,WAAWA,CAAA,EAAG;MACZ,MAAMF,WAAW,GAAG,IAAI,CAACA,WAAW;MACpC,MAAMD,OAAO,GAAG,IAAI,CAACA,OAAO;MAC5B,IAAI,CAACF,SAAS,GAAG,IAAI,CAACA,SAAS,CAAC0B,MAAM,CAAC,IAAI,CAACpB,gBAAgB,CAACM,KAAK,CAAC,CAACT,WAAW,GAAG,CAAC,IAAID,OAAO,EAAEC,WAAW,GAAGD,OAAO,CAAC,CAAC;IACzH,CAAC;IACDsB,QAAQA,CAAA,EAAG;MACT,IAAI,CAACrB,WAAW,GAAG,IAAI,CAACA,WAAW,GAAG,CAAC;MACvC,IAAI,CAACE,WAAW,CAAC,CAAC;IA